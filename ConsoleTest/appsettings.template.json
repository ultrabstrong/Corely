{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=3306;Database=dbName;Uid=userName;Pwd=password;"
  },
  "SystemKey": "symmetric-encryption-key",
  "PasswordValidation": {
    "MinimumLength": 6,
    "RequireUppercase": true,
    "RequireLowercase": true,
    "RequireDigit": true,
    "RequireNonAlphanumeric": true
  },
  "SecurityOptions": {
    "MaxLoginAttempts": 5
  }
}
